@model List<WebApplication2.ViewModels.FullDetailEntertainmentEventReadDto>
<h2>Pending Entertainment Events</h2>
@if (ViewBag.Error != null) {
    <div class="alert alert-danger">@ViewBag.Error</div>
}
<table class="table table-dark">
    <tr><th>Performer</th><th>Show Category</th><th>Genre</th><th>Date</th><th>Location</th><th>Seats</th></tr>
    @foreach (var e in Model) {
        <tr>
            <td>@e.performerName</td>
            <td>@e.showCategory</td>
            <td>@e.genre</td>
            <td>@e.day/@e.mounth/@e.year</td>
            <td>@e.location</td>
            <td>@e.aviilableSeats</td>
        </tr>
    }
</table> 